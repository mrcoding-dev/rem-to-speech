{
  "manifestVersion": 1,
  "id": "text-to-speech",
  "name": "Text to Speech",
  "author": "Gabriel",
  "repoUrl": "https://github.com/remnoteio/remnote-official-plugins",
  "version": {
    "major": 1,
    "minor": 1,
    "patch": 0
